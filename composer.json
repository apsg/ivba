{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "anhskohbo/no-captcha": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.5@dev",
        "doctrine/dbal": "^2.5",
        "intervention/image": "^2.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "phpdocumentor/reflection-common": "1.0",
        "sentry/sentry-laravel": "^0.12.0@dev",
        "setasign/fpdi-fpdf": "^1.6",
        "spatie/laravel-fractal": "dev-master",
        "srmklive/paypal": "~1.0",
        "tom-lingham/searchy": "^3.0",
        "tpay-com/tpay-php": "dev-master",
        "vimeo/laravel": "^5.0",
        "yajra/laravel-datatables-oracle": "~8.0",
        "yajra/laravel-disqus": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2@dev",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
